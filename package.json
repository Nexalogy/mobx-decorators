{
  "name": "mobx-decorators",
  "version": "4.1.0",
  "description": "Several helper MobX decorators (setter, observe, save, ...)",
  "repository": "farwayer/mobx-decorators",
  "author": "farwayer <farwayer@gmail.com>",
  "license": "WTFPL",
  "keywords": [
    "mobx",
    "decorator",
    "setter",
    "set",
    "toggle",
    "intercept",
    "observe",
    "save"
  ],
  "main": "lib",
  "jsnext:main": "esm",
  "module": "esm",
  "react-native": "esm/index.react-native.js",
  "scripts": {
    "build": "yarn clean && yarn build-lib && yarn build-esm",
    "build-lib": "babel src --out-dir lib",
    "build-esm": "babel src --out-dir esm --config-file=./.babelrc.esm.js",
    "clean": "rm -rf lib esm",
    "test": "mocha",
    "cover": "istanbul cover _mocha --report lcovonly -- --reporter spec && cat coverage/lcov.info | coveralls && rm -rf coverage",
    "prepack": "yarn build && yarn test"
  },
  "peerDependencies": {
    "mobx": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.44",
    "@babel/plugin-proposal-decorators": "7.0.0-beta.44",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.44",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/register": "^7.0.0-beta.44",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.2",
    "@types/should": "^13.0.0",
    "coveralls": "^3.0.0",
    "istanbul": "1.1.0-alpha.1",
    "mobx": "^4.1.1",
    "mocha": "^5.0.5",
    "should": "^13.2.1",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.44",
    "lodash.difference": "^4.5.0",
    "lodash.intersection": "^4.4.0"
  }
}
