{
  "name": "mobx-decorators",
  "version": "2.3.3",
  "description": "Several helper MobX decorators (setter, observe, save, ...)",
  "repository": "farwayer/mobx-decorators",
  "author": "farwayer <farwayer@gmail.com>",
  "license": "WTFPL",
  "keywords": [
    "mobx",
    "decorator",
    "setter",
    "set",
    "toggle",
    "intercept",
    "observe",
    "save"
  ],
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "react-native": "src/index.react-native.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "test": "mocha --require @babel/register --require should",
    "cover": "istanbul cover _mocha --report lcovonly -- --require @babel/register --require should --reporter spec && cat coverage/lcov.info | coveralls && rm -rf coverage",
    "prepack": "npm run test && npm run build"
  },
  "peerDependencies": {
    "mobx": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.40",
    "@babel/plugin-proposal-decorators": "7.0.0-beta.40",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "coveralls": "^3.0.0",
    "istanbul": "1.1.0-alpha.1",
    "mobx": "^3.1.16",
    "mocha": "^5.0.4",
    "should": "^13.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.40",
    "lodash.difference": "^4.5.0",
    "lodash.intersection": "^4.4.0"
  }
}
